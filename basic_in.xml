   <source name="test1" detail="Example 1 / test1" xpath_expr="/AAA">
      <AAA>
         <BBB/>
         <CCC/>
         <BBB/>
         <BBB/>
         <DDD>
            <BBB/>
         </DDD>
         <CCC/>
     </AAA>      
   </source>
